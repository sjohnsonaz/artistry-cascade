{
  "name": "cascade-components",
  "version": "0.1.2",
  "description": "A powerful component library for Cascade",
  "repository": "https://www.github.com/sjohnsonaz/cascade-components.git",
  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/scripts/modules/CascadeComponents.js",
  "typings": "dist/scripts/modules/CascadeComponents.d.ts",
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "rimraf ./dist && tsc --declaration",
    "test": "tsc && node src/mocha/NodeRunner.js",
    "min": "webpack --config webpack.config.js --progress",
    "dev": "webpack --config webpack.dev.config.js --progress",
    "watch": "webpack --config webpack.dev.config.js --watch --progress",
    "watch-link": "tsc --watch --declaration",
    "node": "node ./dist/scripts/main.js"
  },
  "peerDependencies": {
    "cascade": "^0.1.14",
    "artistry": "^0.1.2"
  },
  "devDependencies": {
    "@types/chai": "4.0.1",
    "@types/mocha": "2.2.41",
    "artistry": "^0.1.2",
    "autoprefixer": "7.1.2",
    "cascade": "^0.1.14",
    "cascade-validation": "^0.0.1",
    "chai": "4.1.0",
    "css-loader": "0.28.4",
    "es6-shim": "0.35.3",
    "extract-text-webpack-plugin": "3.0.0",
    "jsdom": "11.1.0",
    "mocha": "3.4.2",
    "postcss-loader": "2.0.6",
    "reflect-metadata": "0.1.10",
    "rimraf": "^2.6.2",
    "style-loader": "0.18.2",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-loader": "2.3.1",
    "typescript": "2.8.1",
    "webpack": "3.4.1"
  },
  "keywords": [
    "component",
    "button",
    "modal",
    "input",
    "cascade",
    "reactive",
    "view",
    "viewmodel",
    "mvvm",
    "mvc",
    "template",
    "dom",
    "react",
    "knockout"
  ]
}
